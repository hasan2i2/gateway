{
  "name": "hugenet/gateway",
  "description": "A Laravel package for connecting to all Iraninan payment  gateways",
  "keywords": [
    "laravel",
    "iran",
    "payment",
    "gateway",
    "port",
    "bank",
    "pay",
    "mellat",
    "saman",
    "pasargad",
    "parsian",
    "sadad",
    "meli",
    "payline",
    "jahanpay",
    "zarinpal",
    "iranian-banks",
    "persian-banks",
    "pardakht",
    "dargah",
    "shaparak",
    "poolport",
    "ipay",
    "asan-pardakht",
    "pay-ir",
    "paypal",
    "irankish",
    "saderat"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Hassan mohammadzade",
      "email": "hassan@hugenet.ir"
    },
    {
      "name": "Alireza Khosropoor",
      "email": "alirezakh@hugenet.ir"
    }
  ],
  "require": {
    "nesbot/carbon": "*",
    "paypal/rest-api-sdk-php": "*",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "hugenet\\Gateway\\": "src/",
      "hugenet\\Gateway\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "hugenet\\Gateway\\GatewayServiceProvider"
      ],
      "aliases": {
        "Gateway": "hugenet\\Gateway\\Gateway"
      }
    }
  }
}
